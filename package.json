{
  "name": "vite-enhance-kit",
  "version": "0.1.0",
  "description": "A modular Vite enhancement toolkit for enterprise-grade frontend engineering",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm build:shared && pnpm build:config && pnpm build:plugins && pnpm build:main",
    "build:shared": "pnpm --filter @vite-enhance/shared build",
    "build:config": "pnpm --filter @vite-enhance/config build", 
    "build:plugins": "pnpm --filter './packages/plugins/*' build",
    "build:main": "pnpm --filter vite-enhance build",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "typecheck": "pnpm -r typecheck",
    "version:check": "node scripts/check-versions.js",
    "version:sync": "node scripts/sync-dependencies.js",
    "version:analyze": "node scripts/analyze-workspace.js",
    "version:update": "changeset version",
    "version:release": "pnpm build && changeset publish",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "release": "node scripts/release.js",
    "release:dry-run": "node scripts/release.js --dry-run",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "vite",
    "frontend",
    "build-tool",
    "monorepo",
    "engineering"
  ],
  "author": "Vite Enhance Kit Team",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "glob": "catalog:",
    "js-yaml": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.5.0"
  },
  "packageManager": "pnpm@9.5.0"
}