{
  "name": "vite-enhance-kit",
  "version": "0.1.0",
  "description": "A modular Vite enhancement toolkit for enterprise-grade frontend engineering",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "typecheck": "pnpm -r typecheck",
    "version:check": "node scripts/check-versions.js",
    "version:sync": "node scripts/sync-dependencies.js",
    "version:analyze": "node scripts/analyze-workspace.js",
    "version:update": "changeset version",
    "version:release": "pnpm build && changeset publish",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status"
