{
  "name": "@vite-enhance/shared",
  "version": "0.1.1",
  "description": "Shared utilities, types, and logger for Vite Enhance Kit",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
