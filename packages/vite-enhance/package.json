  },
  "dependencies": {
    "@vite-enhance/config": "workspace:*",
    "@vite-enhance/shared": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "keywords": [
    "vite",
    "enhance",
    "config",
    "plugin"
