{
  "name": "@vite-enhance/plugin-framework-vue",
  "version": "0.2.0",
  "description": "Vue framework plugin for Vite Enhance Kit with Vue DevTools integration",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "keywords": [
    "vite",
    "vue",
    "plugin",
    "enhance"
  ],
  "author": "Vite Enhance Kit",
  "license": "MIT",
  "dependencies": {
    "@vite-enhance/shared": "workspace:*",
    "@vitejs/plugin-vue": "^6.0.3",
    "vite-plugin-vue-devtools": "^8.0.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue": "^3.5.26"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}