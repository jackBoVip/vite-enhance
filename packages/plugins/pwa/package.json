{
  "name": "@vite-enhance/plugin-pwa",
  "version": "0.1.1",
  "description": "PWA plugin for Vite Enhance Kit",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
