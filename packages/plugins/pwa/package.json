    "pwa",
    "plugin",
    "enhance"
  ],
  "author": "Vite Enhance Kit",
  "license": "MIT",
  "dependencies": {
    "@vite-enhance/shared": "workspace:*",
    "vite-plugin-pwa": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  }
}