{
  "name": "@vite-enhance/plugin-framework-vue",
  "version": "0.2.1",
  "description": "Vue framework plugin for Vite Enhance Kit with Vue DevTools integration",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "vite",
    "vue",
    "plugin",
    "enhance"
  ],
  "author": "Vite Enhance Kit",
  "license": "MIT",
  "dependencies": {
    "@vite-enhance/shared": "workspace:*",
    "@vitejs/plugin-vue": "catalog:",
    "vite-plugin-vue-devtools": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vue": "catalog:"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}